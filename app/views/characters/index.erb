<h1>Characters</h1>
<% @characters.each do |character| %>
  <a href= "/characters/<%= character.id %>"><%= character.name %> - <%= character.char_class %></a><br />
<% end %>
<p>
  <a href="/characters/new">New Character</a>
  <a href="/users/<%= @current_user.slug %>"><%= @current_user.username %> Characters</a>
  <a href="/logout">Logout</a>
</p>
